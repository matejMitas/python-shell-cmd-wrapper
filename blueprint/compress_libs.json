{
	"settings": {
		"required_flags": [
			"@input", "@output"
		],
		"libraries" 	: [
			"kdu_compress", "opj_compress"
		]
	},
	"flags": {
		"@input": [
			{
				"flag"		: "-i",
				"unifier"	: null,
				"format"	: "single"
			},
			{
				"flag"		: "-i",
				"unifier"	: null,
				"format"	: "single"
			}
		],
		"@output": [
			{
				"flag"		: "-o",
				"unifier"	: null,
				"format"	: "single"
			},
			{
				"flag"		: "-o",
				"unifier"	: null,
				"format"	: "single"
			}
		],
		"@blocks": [
			{
				"flag"		: "Cblk",
				"unifier"	: "=",
				"format"	: "pair_braces"
			},
			{
				"flag"		: "-b",
				"unifier"	: null,
				"format"	: "pair"
			}
		],
		"@precincts": [
			{
				"flag"		: "Cprecincts",
				"unifier"	: "=",
				"format"	: "pair_braces",
				"list"		: {
					"divider": ","
				}
			},
			{
				"flag"		: "-c",
				"unifier"	: null,
				"format"	: "pair_box",
				"list"		: {
					"divider": ","
				}
			}
		],
		"@mode": [
			{
				"flag"		: "Cmodes",
				"unifier"	: "=",
				"format" 	: "single",
				"list"		: {
					"divider": "|"
				}
			},
			{
				"flag"		: "-M",
				"unifier"	: null,
				"format"	: "match",
				"match"		: {
					"pattern": {
						"BYPASS"	: 1,
						"RESET"		: 2, 
						"RESTART"	: 4,
						"VSC"		: 8,
						"ERTERM"	: 16,
						"SEGMARK"	: 32
					},
					"modifier"	: "+"
				}
			}
		],
		"@compression": [
			{
				"flag"			: "Creversible",
				"unifier"		: "=",
				"format"		: "match",
				"match": {
					"pattern": {
						"lossy"		: "no",
						"lossless"	: "yes"
					}
				}
			},
			{
				"flag"			: "-I",
				"unifier"		: null,
				"format"		: "toggle",
				"toggle": {
					"pattern": ["lossless", "lossy"]
				}
			}
		]
	}
}